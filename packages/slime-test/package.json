{
  "name": "slime-test",
  "version": "0.2.8",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "exports": {
    ".": {
      "development": "./src/index.ts",
      "types": "./dist/index.d.mts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "default": "./dist/index.mjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "prepublishOnly": "npm run build",
    "test:stage1": "tsx --conditions=development src/utils/test-stage1.ts",
    "test:stage2": "tsx --conditions=development src/utils/test-stage2.ts",
    "test:stage3": "tsx --conditions=development src/utils/test-stage3.ts",
    "test:js-stage1": "tsx --conditions=development src/utils/test-js-stage1.ts",
    "test:js-stage2": "tsx --conditions=development src/utils/test-js-stage2.ts",
    "test:js-stage3": "tsx --conditions=development src/utils/test-js-stage3.ts",
    "test": "npm run test:stage1 && npm run test:stage2 && npm run test:stage3",
    "test:js": "npm run test:js-stage1 && npm run test:js-stage2 && npm run test:js-stage3"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "slime-ast": "^0.2.8",
    "slime-babel-test": "^0.0.3",
    "slime-generator": "^0.2.8",
    "slime-parser": "^0.2.8",
    "slime-token": "^0.2.8",
    "subhuti": "^0.2.6"
  },
  "devDependencies": {
    "tsdown": "0.17.0-beta.6",
    "typescript": "^5.8.3"
  },
  "gitHead": "92d3260e10861bdd70401b2dee23cbb8de613900"
}
